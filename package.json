{
  "name": "nextjs-starter",
  "license": "UNLICENSED",
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "create:fc": "hygen new fc",
    "create:pc": "hygen new pc",
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --watch",
    "dev:api": "aspida --watch",
    "build": "pathpida && aspida && next build",
    "generate": "yarn build && next export",
    "start": "next start",
    "test": "jest",
    "fix": "prettier --write './**/*.{js,jsx,ts,tsx}'",
    "lint": "next lint . --cache",
    "lint:fix": "next lint . --cache --fix",
    "lint-staged": "lint-staged",
    "tsc": "tsc",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aspida/axios": "1.7.1",
    "axios": "0.21.1",
    "focus-visible": "5.2.0",
    "framer-motion": "4.1.17",
    "next": "11.0.1",
    "next-seo": "4.26.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-spinners": "0.11.0",
    "react-use": "17.2.4",
    "tailwindcss": "2.2.7"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "11.0.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@types/jest": "27.0.0",
    "@types/react": "17.0.15",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "autoprefixer": "10.3.1",
    "eslint": "7.32.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "7.0.1",
    "hygen": "6.1.0",
    "jest": "27.0.6",
    "lint-staged": "11.1.1",
    "next-compose-plugins": "2.2.1",
    "npm-run-all": "4.1.5",
    "pathpida": "0.15.4",
    "postcss": "8.3.6",
    "prettier": "2.3.2",
    "ts-jest": "27.0.4",
    "typescript": "4.3.5"
  }
}
